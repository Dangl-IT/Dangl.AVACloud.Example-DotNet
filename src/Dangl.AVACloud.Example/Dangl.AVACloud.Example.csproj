<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <!-- This is the public package on the NuGet feed. If you have access to the evaluation package,
    you can directly reference 'Dangl.AVACloud.Client' and get access to the actual model implementations
    with more features such as auto-calculation-->
    <PackageReference Include="Dangl.AVACloud.Client.Public" Version="1.18.5" />
  </ItemGroup>

  <ItemGroup>
    <None Update="GAEBXML_EN.X86">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
